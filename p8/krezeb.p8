pico-8 cartridge // http://www.pico-8.com
version 16
__lua__
function is_sprite(value,sprites)
  for _,sprite in pairs(sprites) do
    if sprite == value then
      return true
    end
  end
  return false
end

function move_object(object,x,y,dir)
  local tx,ty = object.x+x,object.y+y
  local tile = mget(tx/8+0.5,ty/8+0.5)
  if is_sprite(tile,sprite_empty) then
    object.x,object.y = tx,ty
  end
  if dir then
    object.dir = dir
  end
  return tile
end

function distance(a,b)
  return sqrt((a.x-b.x)^2+(a.y-b.y)^2)
end

function collide(a,b,td)
  d = distance(a,b)
  return d > 0 and d < td
end


_dirs={{-1,0},{1,0},{0,-1},{0,1}}
function dir_to_vec(dir)
  local vec = _dirs[dir+1]
  return vec[1],vec[2]
end

function move_towards(a,b)
  local tx,ty = 0,0
  if distance(a,b) < 64 then
    if a.x < b.x then tx = 1 end
    if a.x > b.x then tx = -1 end
    if a.y < b.y then ty = 1 end
    if a.y > b.y then ty = -1 end
  end
  return tx,ty
end

-- setup

sprite_player = 1
sprite_bullet = 2
sprite_empty = {0,53}
sprite_monster = {3,4,5}

width=128
height=128

bullets = {}
player = {dir = 0}
monsters = {}
for x = 1,width do
  for y = 1,height do
    local sprite = mget(x,y)
    if sprite == sprite_player then
      player.x,player.y = x*8,y*8
      mset(x,y,0)
    elseif is_sprite(sprite,sprite_monster) then
      add(monsters,{x=x*8,y=y*8,spr=sprite})
      mset(x,y,0)
    end
  end
end

-- main function

function _draw()
  cls()
  local ox = width/2-player.x
  local oy = height/2-player.y

  -- map

  --map(0,0,width,height,ox/8,oy/8)
  map(0,0,ox,oy)

  -- player

  spr(sprite_player,player.x+ox,player.y+oy)
  if not player.dead then
    local tx,ty = 0,0
    if btn(0) then move_object(player,-1,0,0) end
    if btn(1) then move_object(player,1,0,1) end
    if btn(2) then move_object(player,0,-1,2) end
    if btn(3) then move_object(player,0,1,3) end
    if btnp(4) then
      add(bullets,{x=player.x,y=player.y,dir=player.dir})
    end
  end

  -- monsters

  for _,monster in pairs(monsters) do
    spr(monster.spr,monster.x+ox,monster.y+oy)
    local tx,ty = move_towards(monster,player)
    move_object(monster,0,ty/4)
    move_object(monster,tx/4,0)
    if collide(monster,player,4) then
      player.dead = true
    end
  end

  -- bullets

  for _,bullet in pairs(bullets) do
    spr(sprite_bullet,bullet.x+ox,bullet.y+oy)
    local tx,ty = dir_to_vec(bullet.dir)
    local tile = move_object(bullet,tx*2,ty*2)
    if is_sprite(tile,sprite_empty) then
      for _,monster in pairs(monsters) do
        if collide(bullet,monster,8) then
          del(bullets,bullet)
          del(monsters,monster)
        end
      end
    else
      del(bullets,bullet)
    end
  end

  -- gui

  if player.dead then
    print("you lose")
  elseif #monsters == 0 then
    print("you win")
  end

end

